CREATE DATABASE sistema_boletos;

USE sistema_boletos;

CREATE TABLE clientes (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nome VARCHAR(100) NOT NULL,
    cpf VARCHAR(14) NOT NULL UNIQUE,
    data_nascimento DATE NOT NULL,
    nome_mae VARCHAR(100) NOT NULL
);

